<div class="container">
<h1>Add new review</h1>

 <%= simple_form_for [ restaurant, review ] do |f| %>

  <%= f.input:rating , input_html: { min: '0', max: '5' } %>
  <%= f.input:content %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>

<%= link_to 'Back', restaurants_path %>
</div>
